generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model task {
  uuid      String   @id
  title     String   @db.VarChar(255)
  message   String   @db.LongText
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
